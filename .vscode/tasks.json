{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build NinjaTrader Custom",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "NinjaTrader.Custom.csproj",
                "-c",
                "Debug"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            }
        },
        {
            "label": "Rebuild NinjaTrader Custom",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "NinjaTrader.Custom.csproj",
                "-c",
                "Debug",
                "--force"
            ],
            "problemMatcher": "$msCompile",
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            }
        }
    ]
}
